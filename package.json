{
  "name": "@santosrj/kyndra",
  "version": "1.0.1",
  "description": "Data Structures and Algorithms make in Typescript",
  "main": "build/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "reinstall": "pnpm dlx rimraf node_modules && pnpm install --recursive --frozen-lockfile --strict-peer-dependencies",
    "prepare": "husky install",
    "---------- Lint and Format ---------": "",
    "lint": "eslint src/**/*.ts",
    "---------- Tests ---------": "",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "---------- Build ---------": "",
    "build": "pnpm dlx rimraf ./build && tsc"
  },
  "engines": {
    "node": "^18.15.0",
    "npm": "^9.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santos-rj/kyndra.git"
  },
  "files": [
    "build",
    "LICENSE",
    "README.md",
    "package.json",
    "pnpm-lock.yaml"
  ],
  "keywords": [
    "data",
    "structures",
    "algorithms"
  ],
  "author": "Rafael Santos <contato@santosrj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/santos-rj/kyndra/issues"
  },
  "homepage": "https://github.com/santos-rj/kyndra#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@faker-js/faker": "^8.0.2",
    "@jest/globals": "^29.6.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
